
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SofthemeLogin</title>
    @Styles.Render("~/Styles/css")
    @Styles.Render("~/Styles/registration")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
</head>
<body>
   
   
    <div class="container">
        <div class="registration-wrapper">
            <div class="registration-parallelogram">
                <div class="registration-inner-inner-parallelogram">
                </div>

                <div class="registration-inner-parallelogram">
                    @RenderSection("Title")
                    <div class="registration-inner-inner-parallelogram2">
                    </div>

                </div>

                <div class="registration-form">
                    @RenderBody()
                </div>
            </div>
            <div class="registration-logo">
                <a href="@Url.Action("Index", "Home")">
                    <img width="162px" src="~/Content/images/logo-gray.png" />
                </a>

                
                <!-- 552 596 -->
                <hr id="hr-style">
              
                @RenderSection("Menu")


            </div>
        </div>
    </div>
</body>
</html>
